{
    "name": "golauth",
    "active": true,
    "proxy": {
        "preserve_host": false,
        "listen_path": "/api/security/*",
        "upstreams": {
            "balancing": "roundrobin",
            "targets": [
                {
                    "target": "http://golauth:8080"
                }
            ]
        },
        "strip_path": true,
        "append_path": true,
        "methods": [
            "OPTIONS", "GET", "POST"
        ]
    }
}
