{
    "name": "dummy",
    "active": true,
    "proxy": {
        "preserve_host": false,
        "listen_path": "/dummy/*",
        "upstreams": {
            "balancing": "roundrobin",
            "targets": [
                {
                    "target": "http://dummy_service:80"
                }
            ]
        },
        "strip_path": true,
        "append_path": true,
        "methods": [
            "GET"
        ]
    },
    "health_check": {
        "url": "http://dummy_service:8080/status",
        "timeout": 3
    }
}